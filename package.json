    },
    {
      "name": "Our *awesome* community!",
      "url": "https://github.com/Fred-Barclay/Termination/blob/master/AUTHORS"
    }
  ],
  "homepage": "https://atom.io/packages/termination",
  "repository": "https://github.com/Fred-Barclay/Termination",
