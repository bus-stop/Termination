  "providedServices": {
    "TerminationTerminal": {
      "description": "Termination API",
      "versions": {
        "1.1.0": "provideTerminationTerminal"
      }
    },
    "runInTerminal": {
      "description": "Allow to run commands in terminal.",
      "versions": {
        "0.14.5": "provideRunInTerminal"
