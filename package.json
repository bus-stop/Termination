  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0",
    "node-pty-multiarch": "^0.8.1",
    "term.js": "https://github.com/jeremyramin/term.js/tarball/master",
    "underscore": "^1.8.3"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
